<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://getahead.org/dwr//dwr30.dtd">

<dwr>
  <allow>
    <create creator="new" javascript="MultiplePrinter">
      <param name="class" value="jp.ac.oit.igakilab.dwr.multiple.MultiplePrinter"/>
    </create>
    <create creator="new" javascript="WeightTransition">
      <param name="class" value="jp.ac.oit.igakilab.dwr.multiple.WeightTransition"/>
    </create>
    <create creator="new" javascript="WebMemberManager">
      <param name="class" value="jp.ac.oit.igakilab.labshop.webservice.WebMemberManager"/>
    </create>
    <create creator="new" javascript="WebItemManager">
      <param name="class" value="jp.ac.oit.igakilab.labshop.webservice.WebItemManager"/>
    </create>
    <create creator="new" javascript="WebAccountManager">
      <param name="class" value="jp.ac.oit.igakilab.labshop.webservice.WebAccountManager"/>
    </create>
    <!-- テスト用 -->
    <create creator="new" javascript="TestDBCounter">
      <param name="class" value="jp.ac.oit.igakilab.labshop.webservice.test.TestDBCounter"/>
    </create>
    <create creator="new" javascript="TestAggregateAccount">
      <param name="class" value="jp.ac.oit.igakilab.labshop.webservice.test.TestAggregateAccount"/>
    </create>
    <create creator="new" javascript="TestDebugLog">
      <param name="class" value="jp.ac.oit.igakilab.labshop.test.TestDebugLog"/>
    </create>
    <convert converter="bean" match="jp.ac.oit.igakilab.dwr.multiple.*" />
    <convert converter="exception" match="java.lang.Exception" />
    <convert converter="bean" match="java.lang.StackTraceElement"/>
    <convert converter="bean" match="jp.ac.oit.igakilab.labshop.webservice.forms.*"/>
    <!-- テスト用 -->
   	<convert converter="bean" match="jp.ac.oit.igakilab.labshop.dbcontroller.DBCounter"/>
  </allow>
</dwr>
